{
   "version": 2,
   "builds": [{ "src": "index.js", "use": "@vercel/node" }],
   "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
   "env": {
     "NODE_ENV": "@node-env",
     "DB_USER": "@resturant-db-user",
     "DB_PASSWORD": "@resturant-db-password",
     "DB_HOST": "@resturant-db-host",
     "DB_NAME": "@restaurant-db-name",
     "AUTH_JWT_SECRET": "@auth-jwt-secret"
   }
}